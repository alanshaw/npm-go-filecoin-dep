  "description": "Install the latest go-filecoin binary",
  "main": "src/index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "install": "node src/bin.js",
    "test": "aegir test -t node",
